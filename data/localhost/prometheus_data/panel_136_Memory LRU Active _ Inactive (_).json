{
  "panel_title": "Memory LRU Active / Inactive (%)",
  "panel_id": 136,
  "description": "Proportion of memory pages in the kernel's active and inactive LRU lists relative to total RAM. Active pages have been recently used, while inactive pages are less recently accessed but still resident in memory",
  "targets": [
    {
      "refId": "A",
      "query": "(node_memory_Inactive_bytes{instance=\"localhost:9100\",job=\"node_exporter\"}) \n/ \n(node_memory_MemTotal_bytes{instance=\"localhost:9100\",job=\"node_exporter\"})",
      "legendFormat": "Inactive – Less recently used memory, more likely to be reclaimed",
      "hidden": false,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    },
    {
      "refId": "B",
      "query": "(node_memory_Active_bytes{instance=\"localhost:9100\",job=\"node_exporter\"}) \n/ \n(node_memory_MemTotal_bytes{instance=\"localhost:9100\",job=\"node_exporter\"})\n",
      "legendFormat": "Active – Recently used memory, retained unless under pressure",
      "hidden": false,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    }
  ]
}