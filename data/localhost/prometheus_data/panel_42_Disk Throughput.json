{
  "panel_title": "Disk Throughput",
  "panel_id": 42,
  "description": "Disk I/O throughput per device",
  "targets": [
    {
      "refId": "A",
      "query": "irate(node_disk_read_bytes_total{instance=\"localhost:9100\",job=\"node_exporter\",device=~\"[a-z]+|nvme[0-9]+n[0-9]+|mmcblk[0-9]+\"}[5m])",
      "legendFormat": "{{device}} - Read",
      "hidden": false,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    },
    {
      "refId": "B",
      "query": "irate(node_disk_written_bytes_total{instance=\"localhost:9100\",job=\"node_exporter\",device=~\"[a-z]+|nvme[0-9]+n[0-9]+|mmcblk[0-9]+\"}[5m])",
      "legendFormat": "{{device}} - Write",
      "hidden": false,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    }
  ]
}