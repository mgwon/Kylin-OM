{
  "panel_title": "Hardware Fan Speed",
  "panel_id": 325,
  "description": "Displays the current fan speeds (RPM) from hardware sensors via the hwmon interface",
  "targets": [
    {
      "refId": "A",
      "query": "node_hwmon_fan_rpm{instance=\"localhost:9100\",job=\"node_exporter\"} * on(chip) group_left(chip_name) node_hwmon_chip_names{instance=\"localhost:9100\",job=\"node_exporter\"}",
      "legendFormat": "{{ chip_name }} {{ sensor }}",
      "hidden": false,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    },
    {
      "refId": "B",
      "query": "node_hwmon_fan_min_rpm{instance=\"localhost:9100\",job=\"node_exporter\"} * on(chip) group_left(chip_name) node_hwmon_chip_names{instance=\"localhost:9100\",job=\"node_exporter\"}",
      "legendFormat": "{{ chip_name }} {{ sensor }} rpm min",
      "hidden": true,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    }
  ]
}