{
  "panel_title": "Pressure Stall Information",
  "panel_id": 322,
  "description": "How often tasks experience CPU, memory, or I/O delays. “Some” indicates partial slowdown; “Full” indicates all tasks are stalled. Based on Linux PSI metrics:\nhttps://docs.kernel.org/accounting/psi.html",
  "targets": [
    {
      "refId": "CPU some",
      "query": "rate(node_pressure_cpu_waiting_seconds_total{instance=\"localhost:9100\",job=\"node_exporter\"}[5m])",
      "legendFormat": "CPU - Some",
      "hidden": false,
      "result": {
        "status": "success",
        "data": {
          "resultType": "matrix",
          "result": []
        }
      }
    },
